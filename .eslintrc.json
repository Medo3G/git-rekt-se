{
  "extends": "eslint-config-airbnb-base",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-console": 0,
    "no-unused-vars": ["warn", {
      "argsIgnorePattern": "next|res"
    }],
    "no-param-reassign": 0,
    "no-underscore-dangle": 0
  }
}
